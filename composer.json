{
    "name": "rnr1721/le7-links",
    "description": "PSR LinkInterface implementation for le7 MVC framework or any PHP8 PSR project",
    "keywords": ["le7", "le7-framework", "psr13", "links", "component"],
    "authors": [
        {
            "name": "Eugeny G",
            "email": "rnr1721@gmail.com",
            "homepage": "https://xhamster.biz.ua",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Core\\": "src"
        }
    },
    "provide": {
        "psr/link-implementation": "2.0"
    },
    "require": {
        "php": ">=8.1",
        "psr/link": "^2.0",
        "psr/http-message": "^2.0"
    },
    "require-dev": {
        "vimeo/psalm": "^5.7",
        "phpunit/phpunit": "^10.0"
    },
    "scripts": {
        "test": [
            "@phpunit",
            "@psalm"
        ],
        "phpunit": "phpunit",
        "psalm": "psalm"
    }
}
